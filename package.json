{
  "name": "touch-typer",
  "description": "A cool little app to help your touch typing skills",
  "author": "Robert Koch <robert@kochie.io>",
  "main": "main/index.js",
  "productName": "Touch Typer",
  "version": "1.9.4",
  "files": [
    "main/",
    "renderer/out"
  ],
  "scripts": {
    "clean": "rimraf dist main renderer/out renderer/.next",
    "dev": "npm run build-electron && electron .",
    "build-renderer": "next build renderer",
    "build-electron": "esbuild electron-src/index.ts electron-src/preload.ts --loader:.txt=file --outdir=main --platform=node --format=cjs --tsconfig=electron-src/tsconfig.json",
    "build": "npm run build-renderer && npm run build-electron",
    "pack-app": "npm run build && electron-builder --dir",
    "dist": "npm run build && electron-builder -mlw",
    "type-check": "tsc -p ./renderer/tsconfig.json && tsc -p ./electron-src/tsconfig.json",
    "postinstall": "electron-builder install-app-deps",
    "publish": "electron-builder -mlw -p always",
    "lint": "prettier --write ."
  },
  "dependencies": {
    "@apollo/client": "3.8.0-alpha.15",
    "@apollo/experimental-nextjs-app-support": "^0.1.0",
    "@electron/notarize": "^1.2.3",
    "@electron/osx-sign": "^1.0.4",
    "@fortawesome/fontawesome-svg-core": "^6.4.0",
    "@fortawesome/free-regular-svg-icons": "^6.4.0",
    "@fortawesome/free-solid-svg-icons": "^6.4.0",
    "@fortawesome/pro-duotone-svg-icons": "^6.4.0",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@graphql-tools/schema": "^10.0.0",
    "@headlessui/react": "^1.7.14",
    "@heroicons/react": "^2.0.18",
    "@next/mdx": "^13.4.4",
    "@sentry/electron": "^4.5.0",
    "@sentry/nextjs": "^7.53.1",
    "@tailwindcss/forms": "^0.5.3",
    "aws-amplify": "^5.2.4",
    "aws-appsync-auth-link": "^3.0.7",
    "aws-crt": "^1.15.17",
    "deepmerge": "^4.3.1",
    "electron-builder-notarize": "^1.5.1",
    "electron-is-dev": "^2.0.0",
    "electron-log": "^4.4.8",
    "electron-next": "^3.1.5",
    "electron-updater": "^5.3.0",
    "fathom-client": "^3.5.0",
    "formik": "^2.3.2",
    "lodash": "^4.17.21",
    "next": "^13.4.4",
    "prettier": "^2.8.8",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "swr": "^2.1.5",
    "yup": "^1.2.0"
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.1.1",
    "@types/d3": "^7.4.0",
    "@types/lodash.sample": "^4.2.7",
    "@types/lodash.samplesize": "^4.2.7",
    "@types/luxon": "^3.3.0",
    "@types/node": "^20.2.5",
    "@types/react": "^18.2.7",
    "@types/react-dom": "^18.2.4",
    "autoprefixer": "^10.4.14",
    "d3": "^7.8.4",
    "electron": "^24.4.0",
    "electron-builder": "23.6.0",
    "esbuild": "^0.17.19",
    "eslint": "8.41.0",
    "eslint-config-next": "13.4.4",
    "lodash.samplesize": "^4.2.0",
    "luxon": "^3.3.0",
    "postcss": "^8.4.23",
    "raw-loader": "^4.0.2",
    "rimraf": "^5.0.1",
    "tailwindcss": "^3.3.2",
    "typescript": "^5.0.4",
    "url-loader": "^4.1.1"
  }
}
