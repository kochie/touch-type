{
  "name": "touch-typer",
  "description": "A cool little app to help your touch typing skills",
  "author": "Robert Koch <robert@kochie.io>",
  "main": "main/index.js",
  "productName": "Touch Typer",
  "version": "1.9.4",
  "files": [
    "main/",
    "renderer/out"
  ],
  "license": "GNU GPLv3",
  "scripts": {
    "clean": "rimraf dist main renderer/out renderer/.next",
    "dev": "npm run build-electron && electron .",
    "build-renderer": "next build renderer",
    "build-electron": "esbuild electron-src/electron-next.ts electron-src/index.ts electron-src/preload.ts --loader:.txt=file --outdir=main --platform=node --format=cjs --tsconfig=electron-src/tsconfig.json",
    "build": "npm run build-renderer && npm run build-electron",
    "pack-app": "npm run build && electron-builder --dir",
    "dist": "npm run build && electron-builder -mlw",
    "type-check": "tsc -p ./renderer/tsconfig.json && tsc -p ./electron-src/tsconfig.json",
    "postinstall": "electron-builder install-app-deps",
    "publish": "electron-builder -mlw -p always",
    "lint": "prettier --write ."
  },
  "dependencies": {
    "@apollo/client": "3.10.5",
    "@apollo/experimental-nextjs-app-support": "^0.11.2",
    "@electron/notarize": "^2.3.2",
    "@electron/osx-sign": "^1.3.0",
    "@fortawesome/fontawesome-svg-core": "^6.5.2",
    "@fortawesome/free-regular-svg-icons": "^6.5.2",
    "@fortawesome/free-solid-svg-icons": "^6.5.2",
    "@fortawesome/pro-duotone-svg-icons": "^6.5.2",
    "@fortawesome/react-fontawesome": "^0.2.2",
    "@graphql-tools/schema": "^10.0.4",
    "@headlessui/react": "^2.0.4",
    "@heroicons/react": "^2.1.4",
    "@next/mdx": "^14.2.4",
    "@sentry/electron": "^5.1.0",
    "@sentry/nextjs": "^8.10.0",
    "@tailwindcss/forms": "^0.5.7",
    "app-root-path": "^3.1.0",
    "aws-amplify": "^6.3.7",
    "aws-appsync-auth-link": "^3.0.7",
    "aws-crt": "^1.21.3",
    "deepmerge": "^4.3.1",
    "electron-builder-notarize": "^1.5.2",
    "electron-is-dev": "^3.0.1",
    "electron-log": "^5.1.5",
    "electron-next": "^3.1.5",
    "electron-updater": "^6.2.1",
    "fathom-client": "^3.7.0",
    "formik": "^2.4.6",
    "lodash": "^4.17.21",
    "naughty-words": "^1.2.0",
    "next": "^14.2.4",
    "prettier": "^3.3.2",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "sharp": "^0.33.4",
    "swr": "^2.2.5",
    "yup": "^1.4.0"
  },
  "devDependencies": {
    "@electron-forge/cli": "^7.4.0",
    "@types/app-root-path": "^1.2.8",
    "@types/d3": "^7.4.3",
    "@types/lodash.sample": "^4.2.9",
    "@types/lodash.samplesize": "^4.2.9",
    "@types/luxon": "^3.4.2",
    "@types/naughty-words": "^1.2.3",
    "@types/node": "^20.14.7",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "autoprefixer": "^10.4.19",
    "d3": "^7.9.0",
    "electron": "^31.0.2",
    "electron-builder": "24.13.3",
    "esbuild": "^0.21.5",
    "eslint": "8.57.0",
    "eslint-config-next": "14.2.4",
    "graphql": "^16.8.2",
    "lodash.samplesize": "^4.2.0",
    "luxon": "^3.4.4",
    "postcss": "^8.4.38",
    "raw-loader": "^4.0.2",
    "react-error-boundary": "^4.0.13",
    "rimraf": "^5.0.7",
    "tailwindcss": "^3.4.4",
    "typescript": "^5.5.2",
    "url-loader": "^4.1.1"
  }
}
